{
  "name": "Gestione Appuntamenti",
  "short_name": "App Cliente",
  "description": "Gestione appuntamenti e consensi",
  "start_url": "/pwa",
  "display": "standalone",
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "minimal-ui"
  ],
  "background_color": "#ffffff",
  "theme_color": "#4f46e5",
  "orientation": "any",
  "categories": [
    "healthcare",
    "business",
    "appointments"
  ],
  "lang": "it-IT",
  "dir": "ltr",
  "prefer_related_applications": false,
  "scope": "/",
  "id": "gestione-appuntamenti-client",
  "icons": [
    {
      "src": "/icons/default-app-icon.jpg",
      "sizes": "192x192",
      "type": "image/jpeg",
      "purpose": "any"
    },
    {
      "src": "/icons/default-app-icon.jpg",
      "sizes": "512x512",
      "type": "image/jpeg",
      "purpose": "any"
    },
    {
      "src": "/icons/default-app-icon.jpg",
      "sizes": "maskable",
      "type": "image/jpeg",
      "purpose": "maskable"
    },
    {
      "src": "/icons/flowers-icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/icons/default-app-icon.jpg",
      "sizes": "512x512",
      "type": "image/jpeg",
      "platform": "wide",
      "label": "Home page dell'applicazione"
    },
    {
      "src": "/icons/default-app-icon.jpg",
      "sizes": "512x512",
      "type": "image/jpeg",
      "platform": "narrow",
      "label": "Visualizzazione mobile dell'applicazione"
    }
  ],
  "shortcuts": [
    {
      "name": "Area Cliente",
      "url": "/client-area",
      "description": "Accedi alla tua area personale",
      "icons": [
        {
          "src": "/icons/default-app-icon.jpg",
          "sizes": "96x96",
          "type": "image/jpeg"
        }
      ]
    }
  ],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": [
      "navigate-existing",
      "auto"
    ]
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [],
  "protocol_handlers": [],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "related_applications": []
}